file "softGlue_Register.db"
{
pattern {SIG     ADDR CALC}
{DnCntr-1_PRESET 0x8001D8 'A'}
{DnCntr-2_PRESET 0x800238 'A'}
{DivByN-1_N      0x800208 'A-1'}
{DivByN-2_N      0x800268 'A-1'}
}

file "softGlue_InRegister32.db"
{
pattern {SIG     ADDR_MSW ADDR_LSW}
{UpCntr-1_COUNTS 0x8003D0 0x800328}
{UpCntr-2_COUNTS 0x8003D8 0x800348}
}
