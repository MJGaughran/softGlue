file "softGlue_Input.db"
{
pattern {SIG   ADDR}

{AND-1_IN1       0x8000B0}
{AND-1_IN2       0x8000B8}
{AND-2_IN1       0x8000D0}
{AND-2_IN2       0x8000D8}
{OR-1_IN1        0x8000F0}
{OR-1_IN2        0x8000F8}
{OR-2_IN1        0x800110}
{OR-2_IN2        0x800118}
{XOR-1_IN1       0x800130}
{XOR-1_IN2       0x800138}
{XOR-2_IN1       0x800150}
{XOR-2_IN2       0x800158}
{DEMUX2-1_IN     0x800170}
{DEMUX2-1_SEL    0x800178}
{DEMUX2-2_IN     0x800190}
{DEMUX2-2_SEL    0x800198}
{DFF-1_D         0x800270}
{DFF-1_CLOCK     0x800278}
{DFF-1_SET       0x800280}
{DFF-1_CLEAR     0x800288}
{DFF-2_D         0x8002A0}
{DFF-2_CLOCK     0x8002A8}
{DFF-2_SET       0x8002B0}
{DFF-2_CLEAR     0x8002B8}
{DivByN-1_ENABLE 0x8001e0}
{DivByN-1_CLOCK  0x8001e8}
{DivByN-1_RESET  0x8001f0}
{DivByN-2_ENABLE 0x800240}
{DivByN-2_CLOCK  0x800248}
{DivByN-2_RESET  0x800250}
{DnCntr-1_ENABLE 0x8001B8}
{DnCntr-1_CLOCK  0x8001B0}
{DnCntr-1_LOAD   0x8001C0}
{DnCntr-2_ENABLE 0x800218}
{DnCntr-2_CLOCK  0x800210}
{DnCntr-2_LOAD   0x800220}
{MUX2-1_IN0      0x8002D0}
{MUX2-1_IN1      0x8002D8}
{MUX2-1_SEL      0x8002E0}
{MUX2-2_IN0      0x8002F0}
{MUX2-2_IN1      0x8002F8}
{MUX2-2_SEL      0x800300}
{UpCntr-1_ENABLE 0x800318}
{UpCntr-1_CLOCK  0x800310}
{UpCntr-1_CLEAR  0x800320}
{UpCntr-2_ENABLE 0x800338}
{UpCntr-2_CLOCK  0x800330}
{UpCntr-2_CLEAR  0x800340}
# These are outputs from the user's viewpoint, but they behave like softGlue
# inputs.
{FO1           0x800070}
{FO2           0x800078}
{FO3           0x800080}
{FO4           0x800088}
{FO5           0x800090}
{FO6           0x800098}
{FO7           0x8000A0}
{FO8           0x8000A8}
{FO9           0x800390}
{FO10          0x800398}
{FO11          0x8003A0}
{FO12          0x8003A8}
{FO13          0x8003B0}
{FO14          0x8003B8}
{FO15          0x8003C0}
{FO16          0x8003C8}
}

